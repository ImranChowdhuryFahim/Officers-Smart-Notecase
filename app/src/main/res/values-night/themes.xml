<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OfficersSmartNotecase" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/tb</item>
        <item name="colorSecondaryVariant">@color/tb</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="colorAccent">@color/tb</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.OfficersSmartNotecase.SplashScreen" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
    <style name="Theme.OfficersSmartNotecase.Reg" parent="Theme.AppCompat">
        <item name="colorPrimary">@null</item>

    </style>

</resources>